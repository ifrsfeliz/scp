<%= simple_form_for(@professor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :cpf %>
    <%= f.input :rg %>
    <%= f.input :email %>
    <%= f.input :telefone %>
    <%= f.input :celular %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
